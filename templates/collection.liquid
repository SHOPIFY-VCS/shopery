<div class="max-w-screen-xl mx-auto px-4 sm:px-6 md:px-8 xl:px-0">
  <h1>Collection Page</h1>
  <h3>{{ collection.title }}</h3>
  <br>
  <br>
  {% for product in collection.products %}
    <div>
      <a href="{{product.url}}">
        <h1>{{ product.title }}</h1>
      </a>
    </div>
  {% endfor %}
</div>
